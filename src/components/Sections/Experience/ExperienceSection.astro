---
import { getEntry } from "astro:content";
import ExperienceSectionClient from "./ExperienceSectionClient";
import type { Locale } from "../../../i18n/ui";

interface Props {
  lang: Locale;
}

const { lang } = Astro.props;

const experienceData = await getEntry("experience", lang);
---

{
  experienceData && experienceData.data && (
    <ExperienceSectionClient
      lang={lang}
      data={experienceData.data}
      client:load
    />
  )
}
